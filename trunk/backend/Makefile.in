LIBLIB_FUNCS = $(basename @LIBOBJS@ @ALLOCA@)
else
LIBLIB_FUNCS =
endif

LIBOBJS = $(addprefix ../lib/,$(addsuffix .lo,$(LIBLIB_FUNCS)))

